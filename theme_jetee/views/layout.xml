<template id="custom_header" inherit_id="website.layout" name="Custom Header">

  <!-- Assign an id  -->
  <xpath expr="//div[@id='wrapwrap']/header" position="attributes">
    <attribute name="id">header</attribute>
  </xpath>


</template>
<template id="custom_block" inherit_id="website.layout" name="Custom block">

  <!-- Assign an id  -->
  <xpath expr="//div[@id='wrapwrap']" position="before">
  <div class="social-block">
  <a href="facebook.com/" ><div style="background-color: #325A97 ;"><i class="fa fa-facebook"></i></div></a>
   <a href="twitter.com/" ><div style="background-color: #4099FF ;"><i class="fa fa-twitter"></i></div></a>
     <a href="twitter.com/"><div style="background-color: #007BB5;"><i class="fa fa-linkedin"></i></div></a>
     <a href="twitter.com/"><div style="background-color: red"><i class="fa fa-youtube"></i></div></a>
     <a href="twitter.com/"><div style="background-color: #d32f2f"> <i class="fa fa-google-plus"></i></div></a>
 
  
  </div>
   
  </xpath>


</template>



  



